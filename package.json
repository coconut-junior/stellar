{
  "name": "stellar",
  "productName": "Stellar",
  "version": "1.3.0",
  "description": "Quickly access creative assets and find product info.",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "pack": "electron-builder --dir",
    "lint": "echo \"No linting configured\"",
    "postinstall": "electron-builder install-app-deps",
    "release": "electron-builder -mw --arm64"
  },
  "build": {
    "appId": "design.jbx.stellar",
    "win": {
      "target": [
        "nsis"
      ]
    },
    "mac": {
      "category": "public.app-category.productivity",
      "target": [
        "zip",
        "dmg"
      ]
    }
  },
  "repository": "https://github.com/coconut-junior/pasteboard",
  "keywords": [],
  "author": {
    "name": "Jimmy Blanck",
    "email": "blanckjm@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "@electron/notarize": "^2.4.0",
    "@fingerprintjs/fingerprintjs": "^4.5.0",
    "decompress-zip": "^0.3.3",
    "dialog-node": "^0.2.1",
    "dotenv": "^16.4.5",
    "electron-forge": "^5.2.4",
    "electron-notarize": "^1.2.2",
    "electron-store": "^8.1.0",
    "electron-unhandled": "^5.0.0",
    "electron-updater": "^6.3.4",
    "extract-zip": "^2.0.1",
    "gsap": "^3.12.5",
    "interactjs": "^1.10.27",
    "jquery": "^3.7.1",
    "license": "^1.0.3",
    "node-stream-zip": "^1.15.0",
    "node-xlsx": "^0.24.0",
    "party-js": "^2.2.0",
    "request": "^2.88.2",
    "save": "^2.9.0",
    "universal-tilt.js": "^2.0.6",
    "xlsx": "^0.19.3"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.4.0",
    "electron": "^32.1.2",
    "electron-builder": "^25.0.5",
    "eslint": "^9.10.0",
    "node": "^22.8.0"
  }
}
