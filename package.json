{
  "name": "stellar",
  "productName": "Stellar",
  "version": "1.2.2",
  "description": "Quickly access creative assets and find product info.",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "pack": "electron-builder --dir",
    "lint": "echo \"No linting configured\"",
    "postinstall": "electron-builder install-app-deps",
    "release": "electron-builder -mw --arm64"
  },
  "build": {
    "appId": "design.jbx.stellar",
    "win": {
      "target": [
        "nsis"
      ]
    },
    "mac": {
      "category": "public.app-category.productivity",
      "target": [
        "zip",
        "dmg"
      ]
    }
  },
  "repository": "https://github.com/coconut-junior/pasteboard",
  "keywords": [],
  "author": {
    "name": "Jimmy Blanck",
    "email": "blanckjm@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "@electron/notarize": "^1.2.3",
    "@fingerprintjs/fingerprintjs": "^3.4.1",
    "decompress-zip": "^0.3.3",
    "dialog-node": "^0.2.1",
    "electron-forge": "^5.2.4",
    "electron-notarize": "^1.2.2",
    "electron-store": "^8.1.0",
    "electron-unhandled": "^4.0.1",
    "electron-updater": "^5.3.0",
    "extract-zip": "^2.0.1",
    "gsap": "^3.12.2",
    "jquery": "^3.6.3",
    "license": "^1.0.3",
    "node-stream-zip": "^1.15.0",
    "node-xlsx": "^0.23.0",
    "party-js": "^2.2.0",
    "request": "^2.88.2",
    "xlsx": "^0.19.3"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "electron": "^29.1.0",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "node": "^20.3.1"
  }
}
